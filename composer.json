{
  "name": "miaoxing/user",
  "type": "miaoxing-plugin",
  "description": "Miaoxing User Plugin",
  "keywords": [
    "miaoxing",
    "user"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.2.6",
    "miaoxing/verify-code": "^0.1.6",
    "miaoxing/admin": "^0.3.1",
    "miaoxing/mail": "^0.1.6",
    "miaoxing/nav": "^0.1.6",
    "miaoxing/region": "^0.1.3"
  },
  "require-dev": {
    "miaoxing/dev": "^6.2.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\User\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\User\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "library",
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin",
        "miaoxing/admin"
      ]
    }
  }
}
