{
  "name": "miaoxing/user",
  "type": "miaoxing-plugin",
  "description": "Miaoxing User Plugin",
  "keywords": [
    "miaoxing", "user"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "dev-master@dev",
    "miaoxing/verify-code": "dev-master@dev",
    "miaoxing/admin": "dev-master@dev",
    "miaoxing/mail": "dev-master@dev",
    "miaoxing/nav": "dev-master@dev"
  },
  "require-dev": {
    "miaoxing/coding-standards": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\User\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\User\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
