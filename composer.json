{
  "name": "miaoxing/user",
  "type": "miaoxing-plugin",
  "description": "Miaoxing User Plugin",
  "keywords": [
    "miaoxing",
    "user"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.6.5",
    "miaoxing/verify-code": "^0.1.26",
    "miaoxing/admin": "^0.8.0",
    "miaoxing/mail": "^0.1.26",
    "miaoxing/nav": "^0.1.27",
    "miaoxing/region": "^0.2.8",
    "miaoxing/queue": "^0.1.34"
  },
  "require-dev": {
    "miaoxing/dev": "^8.1.2",
    "nesbot/carbon": "^2.46"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\User\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\User\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 1.x-dev",
      "miaoxing/mail": "miaoxing/mail as 0.1.x-dev",
      "miaoxing/admin": "miaoxing/admin as 0.7.x-dev",
      "miaoxing/services": "miaoxing/services as 0.5.x-dev"
    }
  }
}
