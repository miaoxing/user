{
  "name": "miaoxing/user",
  "type": "miaoxing-plugin",
  "description": "Miaoxing User Plugin",
  "keywords": [
    "miaoxing",
    "user"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.1.7",
    "miaoxing/verify-code": "dev-master@dev",
    "miaoxing/admin": "^0.1.7",
    "miaoxing/mail": "dev-master@dev",
    "miaoxing/nav": "dev-master@dev"
  },
  "require-dev": {
    "miaoxing/dev": "^6.1.1"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\User\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\User\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "library",
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin",
        "miaoxing/admin"
      ]
    }
  }
}
