{
  "name": "miaoxing/user",
  "type": "miaoxing-plugin",
  "description": "Miaoxing User Plugin",
  "keywords": [
    "miaoxing",
    "user"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.4.0",
    "miaoxing/verify-code": "^0.1.18",
    "miaoxing/admin": "^0.4.0",
    "miaoxing/mail": "^0.1.18",
    "miaoxing/nav": "^0.1.18",
    "miaoxing/region": "^0.2.0",
    "miaoxing/queue": "^0.1.26"
  },
  "require-dev": {
    "miaoxing/dev": "^8.0.0",
    "nesbot/carbon": "^2.46"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\User\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\User\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 0.1.x-dev",
      "miaoxing/mail": "miaoxing/mail as 0.1.x-dev",
      "miaoxing/admin": "miaoxing/admin as 0.4.x-dev"
    }
  }
}
