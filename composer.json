{
  "name": "miaoxing/user",
  "type": "miaoxing-plugin",
  "description": "Miaoxing User Plugin",
  "keywords": [
    "miaoxing", "user"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.1.0",
    "miaoxing/verify-code": "dev-master@dev",
    "miaoxing/admin": "^0.1.0",
    "miaoxing/mail": "dev-master@dev",
    "miaoxing/nav": "dev-master@dev"
  },
  "require-dev": {
    "miaoxing/coding-standards": "^5.0.0@beta"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\User\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\User\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "library",
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin",
        "miaoxing/admin"
      ]
    }
  }
}
